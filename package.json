{
  "name": "amizone-bot",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "type": "module",
  "main": "app.js",
  "types": "app.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap \"test/**/*.test.js\"",
    "start": "fastify start -l info app.js",
    "dev": "fastify start -w -l info -P app.js",
    "lint": "eslint . --ext .js,.mjs,.jsx --quiet",
    "fix": "eslint . --ext .js,.mjs,.jsx --quiet --fix",
    "format": "prettier \"./**/*.{js,mjs}\" --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/sensible": "^4.1.0",
    "amizone_api": "0.0.6",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "eslint-plugin-import": "^2.25.3",
    "fastify": "^4.0.0",
    "fastify-cli": "^4.1.1",
    "fastify-plugin": "^3.0.0",
    "mongodb": "^4.7.0",
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.7.1",
    "tap": "^16.1.0"
  }
}
